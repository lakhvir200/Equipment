
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Equipment Management System</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="../../Theme/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="../../Theme/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="../../Theme/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="../../Theme/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">




</head>
<body class="sidebar-collapse">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-dark navbar-gray-dark">
            <!-- Left navbar links -->

            <ul class="navbar-nav">
                <li class="nav-item d-sm-inline-block">
                    <a href="/home/dashboard" class="brand-link">
                        <img height="25" width="25" src="../../Theme/dist/img/logoems.png"
                             alt="Equipment Management System"
                             class="brand-image img elevation-3"
                             style="opacity: .8">
                    </a>
                </li>
                <li class="nav-item dropdown">
                    <a id="dropdownSubMenu1" href="#" data-toggle="dropdown" aria-haspopup="false" aria-expanded="false" class="nav-link dropdown-toggle">Equipments</a>
                    <ul aria-labelledby="dropdownSubMenu1" class="dropdown-menu border-0 shadow " style="left: 0px; right: inherit;">
                        <li><a href="/equipment/AllEquipment" class="dropdown-item">Equipment Management</a></li>
                        <li><a href="/Repair/Index" class="dropdown-item">Equipment Repair</a></li>
                        <li><a href="/Repair/Index" class="dropdown-item">Pending Repair</a></li>
                    </ul>
                </li>
                <li class="nav-item dropdown">
                    <a id="dropdownSubMenu1" href="#" data-toggle="dropdown" aria-haspopup="false" aria-expanded="false" class="nav-link dropdown-toggle">Master</a>
                    <ul aria-labelledby="dropdownSubMenu1" class="dropdown-menu border-0 shadow " style="left: 0px; right: inherit;">
                        @foreach (var item in Equipment.Admin.MVC.UI.Services.Utilities.GetMasterMenu())
                        {
                            <li><a href="/MasterManager/index?q=@Equipment.Admin.MVC.UI.Services.Utilities.Encrypt(item.MasterID.ToString())" class="dropdown-item">@item.Name</a></li>
                        }
                    </ul>
                </li>
            </ul>
            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Notifications Dropdown Menu -->
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i class="far fa-users"></i>
                        <span class="nav-item d-none d-sm-inline-block">
                            @User.Identity.Name (@User.FindFirst(claim => claim.Type == System.Security.Claims.ClaimTypes.Role)?.Value)
                        </span>
                    </a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link" href="/account/Logout">
                        <i class="far fa-users"></i>
                        <span class="nav-item d-none d-sm-inline-block">
                            Log Out
                        </span>
                    </a>
                </li>
            </ul>
        </nav>
        @RenderBody()
        <footer class="main-footer">
            <div class="float-right d-none d-sm-block">
                <b>Version</b> 3.0.5
            </div>
            <strong>Copyright &copy; 2014-@System.DateTime.Now.Year <a href="#">ZOOMVE</a>.</strong> All rights
            reserved.
        </footer>
    </div>
    <script src="../../Theme/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="../../Theme/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- DataTables -->
    <script src="../../Theme/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="../../Theme/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="../../Theme/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="../../Theme/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>

    <!-- AdminLTE App -->
    <script src="../../Theme/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="../../Theme/dist/js/demo.js"></script>
